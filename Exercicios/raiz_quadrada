#Raiz quadrada
.data 0x10008000
num: .word 2
res: .word 0
bit: .word 0
resl: .space 1 

.text
.globl main

main: #inicio
	la $t0, num #carrega o valor de num para calcular a raiz quadrada
	lw $a0, 0($t0)
	jal raizquadrada #chama a função
	
	
	
	j exit #vai para o final do programa

raizquadrada:
	la $t1,res
	lw $s0, 0($t1)
	la $t2,bit
	lw $s1, 0($t2)
	
	move $v0, $s0
	jr $ra #retorna para a main

exit: #FIM
